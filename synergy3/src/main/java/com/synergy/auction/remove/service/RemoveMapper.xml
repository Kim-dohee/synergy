<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.synergy.auction.remove.service.RemoveMapper">
	<insert id="removeInsert"
			parameterType="com.synergy.auction.remove.service.RemoveDto">
		INSERT INTO remove(
			remove_id
			,remove_date
		) VALUES (
			#{Id}
			,now()
		)
	</insert>

</mapper>